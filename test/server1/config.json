{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 8000,
  "legacyExplorer": true,
  "authorization": {
    "enabled": true,
    "options": {
      "models": {
        "User": "WeflexUser",
        "AccessToken": "WeflexAccessToken"
      }
    }
  },
  "socketioEnabled": true,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": true,
      "xml": false
    },
    "json": {
      "strict": true,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": {
      "origin": true,
      "credentials": true
    },
    "errorHandler": {
      "disableStackTrace": true
    }
  }
}
